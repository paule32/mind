	"jan", { "Januar"	: "max", "jan": "min" },
	"feb", { "Februar"	: "max", "feb": "min" },
	"mar", { "März" 	: "max", "mär": "min" },
	"may", { "Mai"		: "max", "mai": "min" },
	"jun", { "Juni" 	: "max", "jun": "min" },
	"jul", { "July" 	: "max", "jul": "min" },
	"aug", { "August"	: "max", "aug": "min" },
	"sep", { "September"	: "max", "sep": "min" },
	"oct", { "Oktober"	: "max", "okt": "min" },
	"nov", { "November"	: "max", "nov": "min" },
	"dec", { "Dezember"	: "max", "dez": "min" }
]
